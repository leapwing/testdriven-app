{
    "containerDefinitions": [{
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "testdriven-scores-prod",
                "awslogs-region": "ap-southeast-1"
            }
        },
        "portMappings": [{
            "hostPort": 0,
            "protocol": "tcp",
            "containerPort": 5000
        }],
        "environment": [{
                "name": "APP_SETTINGS",
                "value": "project.config.ProductionConfig"
            },
            {
                "name": "DATABASE_TEST_URL",
                "value": "postgres://postgres:postgres@scores-db:5432/scores_test"
            },
            {
                "name": "DATABASE_URL",
                "value": "%s"
            },
            {
                "name": "USERS_SERVICE_URL",
                "value": "http://testdriven-production-alb-692597783.ap-southeast-1.elb.amazonaws.com"
            }, {
                "name": "EXERCISES_SERVICE_URL",
                "value": "http://testdriven-production-alb-692597783.ap-southeast-1.elb.amazonaws.com"
            }
        ],
        "memoryReservation": 300,
        "image": "%s.dkr.ecr.ap-southeast-1.amazonaws.com/test-driven-scores:production",
        "essential": true,
        "name": "scores"
    }],
    "family": "testdriven-scores-prod-td"
}